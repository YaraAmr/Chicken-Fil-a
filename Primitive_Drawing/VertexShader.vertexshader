#version 330 core

// In data
layout(location = 0) in vec3 vertexPosition_modelspace; 
layout(location = 1) in vec2 texCoord;


// Out data
out vec2 TexCoord;
uniform mat4 MVP;
void main(){
    //gl_Position.xyz = vertexPosition_modelspace;
    //gl_Position.w = 1.0; 
	gl_Position =  MVP * vec4(vertexPosition_modelspace, 1.0);
	TexCoord = texCoord;
}

